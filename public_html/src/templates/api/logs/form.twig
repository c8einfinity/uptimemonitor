<form id="logForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
        <div class="form-group col-md-6">
            <label for="id">Id</label>
            <input class="form-control" type="text" id="id" name="id" placeholder="Id" value="{{ data.id }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="serverId">Server Name</label>
            <input class="form-control" type="text" id="serverName" name="serverName" placeholder="Server Name" value="{{ data.serverName }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="createdAt">Created At</label>
            <input class="form-control" type="text" id="createdAt" name="createdAt" placeholder="Created At" value="{{ data.createdAt }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="monitorType">Monitor Type</label>
            <input class="form-control" type="text" id="monitorType" name="monitorType" placeholder="Monitor Type" value="{{ data.monitorType }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="statusCode">Status Code</label>
            <input class="form-control" type="text" id="statusCode" name="statusCode" placeholder="Status Code" value="{{ data.statusCode }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="rawResult">Raw Result</label>
            <input class="form-control" type="text" id="rawResult" name="rawResult" placeholder="Raw Result" value="{{ data.rawResult }}" disabled>
        </div>
    </div>
    {{"logForm" | formToken | raw }}
</form>
