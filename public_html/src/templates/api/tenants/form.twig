<form id="tenantForm" enctype="multipart/form-data" xmlns="http://www.w3.org/1999/html">
    <div class="row">
        <div class="form-group col-md-6">
            <label for="id">Id</label>
            <input class="form-control" type="text" id="id" name="id" placeholder="Id" value="{{ data.id }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="tenantName">Workspace Name</label>
            <input class="form-control" type="text" id="tenantName" name="tenantName" placeholder="Workspace Name" value="{{ data.tenantName }}">
        </div>
        <div class="form-group col-md-6">
            <label for="createdAt">Created At</label>
            <input class="form-control" type="text" id="createdAt" name="createdAt" placeholder="Created At" value="{{ data.createdAt }}" disabled>
        </div>
        <div class="form-group col-md-6">
            <label for="updatedAt">Updated At</label>
            <input class="form-control" type="text" id="updatedAt" name="updatedAt" placeholder="Updated At" value="{{ data.updatedAt }}" disabled>
        </div>
    </div>
    {{"tenantForm" | formToken | raw }}
</form>
